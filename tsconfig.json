{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es2022",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "plugins": [{ "name": "typescript-plugin-css-modules" }]
    },
    "exclude": ["node_modules"]
}